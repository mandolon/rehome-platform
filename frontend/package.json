{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.0.0",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:a11y": "eslint src --max-warnings=0",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run --reporter=verbose",
    "test:ui": "vitest --ui",
    "test:ui:requests": "vitest run src/app/requests/**/__tests__/*.test.tsx",
    "test:run": "vitest run",
    "test:ci": "vitest run --reporter=dot --silent",
    "test:requests": "vitest run src/app/requests/**/__tests__/*.test.tsx",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "format": "prettier --write .",
    "health:api": "node -e \"console.log('API Health Check URL: http://127.0.0.1:9000'); console.log('Use snippets/health-checks.http in VS Code for full smoke tests');\"",
    "csrf:init": "node -e \"console.log('CSRF Init: Use GET /sanctum/csrf-cookie before login'); console.log('See snippets/health-checks.http for full flow');\"",
    "dev:up": "node -e \"console.log('Starting backend (9000) + frontend (3000)...'); console.log('Backend: cd backend && php artisan serve --port=9000'); console.log('Frontend: npm run dev'); console.log('Use: npm run health:api to test endpoints');\""
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "next": "14.2.32",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.5.4",
    "@radix-ui/react-slot": "^1.2.3",
    "@storybook/addon-a11y": "^9.1.8",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-backgrounds": "^9.0.8",
    "@storybook/addon-controls": "^9.0.8",
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/addon-outline": "^9.0.8",
    "@storybook/addon-themes": "^9.1.8",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/addon-vitest": "^9.1.8",
    "@storybook/react-vite": "^9.1.8",
    "@storybook/test": "^8.6.14",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^9.36.0",
    "eslint-config-next": "14.2.32",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "storybook": "^9.1.8",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "whatwg-fetch": "^3.6.20"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
